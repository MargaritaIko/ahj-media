{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": "airbnb-base",
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest"
    },
    "rules": {
        "linebreak-style": 0,
            "no-restricted-syntax": [
                "error",
                "LabeledStatement",
                "WithStatement"
            ],
            "no-param-reassign": 0,
            "no-unused-vars": 0
      },
      "overrides": [
        {
          "files": ["**/*.test.js"],
          "plugins": ["jest"],
          "extends": ["plugin:jest/recommended"],
          "rules": { "jest/prefer-expect-assertions": "off" }
        }
      ]
    }
    